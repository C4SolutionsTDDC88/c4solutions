{
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "globals": {
      "define": true,
      "localStorage": true,
      "sessionStorage": true,
      "defined": true,
      "location": true,
      "require": true,
      "window": true,
      "document": true,
      "Image": true
    },
    "extends": "airbnb/base",
    "rules": {
      "linebreak-style": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-amd": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "object-shorthand": 0,
      "no-console": 0
    }
  }
  